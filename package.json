{
  "name": "projectile",
  "displayName": "projectile",
  "description": "projectile is an alternatives of emacs projectile.",
  "version": "0.1.1",
  "publisher": "hackartist",
  "author": {
    "name": "hackartist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hackartists/vscode.projectile.git"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "enableProposedApi": true,
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:projectile.projectFiles",
    "onCommand:projectile.projects"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "projectile.projectFiles",
        "title": "Projectile: Files in Project"
      },
      {
        "command": "projectile.projects",
        "title": "Projectile: List projects"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.52.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.15.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "find-git-root": "^1.0.4",
    "git-ls-files": "^1.1.0"
  }
}
